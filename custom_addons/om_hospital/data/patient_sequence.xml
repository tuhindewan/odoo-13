<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="hospital_patient_sequence" model="ir.sequence">
            <field name="name">Hospital Patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="hospital_appointment_sequence" model="ir.sequence">
            <field name="name">Hospital Appointment Sequence</field>
            <field name="code">hospital.appointment.sequence</field>
            <field name="prefix">HA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="hospital_doctor_sequence" model="ir.sequence">
            <field name="name">Hospital Doctor Sequence</field>
            <field name="code">hospital.doctor.sequence</field>
            <field name="prefix">HD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
